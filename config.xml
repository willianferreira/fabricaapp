<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fabrica da Alegria</name>
    <description>
        Fabrica da Alegria, Loja de Eletronicos no Camelodromo de Londrina
    </description>
    <author email="contato@creativenegocios.com" href="http://www.creativenegocios.com">
        Creative Negócios
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.Notification" />
     </feature>
<plugin spec=“https://github.com/apache/cordova-plugin-dialogs.git” source=“git” />
<gap:plugin name="org.apache.cordova.vibration" source="npm" />
</widget>
